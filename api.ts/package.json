{
  "name": "home-network-admin",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc -w",
    "build-and-install": "npm --prefix webui run build && rm -rf public && mkdir public && cp -r webui/build/* public/",
    "jest": "jest src/",
    "jest-watch": "jest --watch src/",
    "jest-watch-all": "jest --watchAll src/",
    "lint": "eslint src --ext .js,.ts",
    "lint-fix": "eslint --fix src --ext .js,.ts",
    "nodemon": "nodemon -w ./dist ./dist/index.js",
    "start": "node ./dist/index.js",
    "test": "npm run lint && npm run jest",
    "test-watch": "npm run jest-watch",
    "test-watch-all": "npm run jest-watch-all"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.0",
    "debug": "^2.6.0",
    "debug-logger": "^0.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.0",
    "fs-extra": "^9.0.0",
    "http-error-constructor": "^0.1.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "notp": "^2.0.0",
    "qrcode": "^1.4.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.0",
    "request-promise-native": "^1.0.0",
    "socket.io": "^2.3.0",
    "thirty-two": "^1.0.0",
    "tough-cookie": "^3.0.0",
    "yaml": "^1.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/lodash": "^4.14.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.14.0",
    "@types/socket.io": "^2.1.13",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "jest": "^26.6.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.2.0"
  }
}
